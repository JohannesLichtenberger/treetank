	<modelVersion>4.0.0</modelVersion>
	<groupId>treetank</groupId>
	<artifactId>builder</artifactId>
	<packaging>pom</packaging>
	<name>treetank-builder</name>
	<version>5.3.4-SNAPSHOT</version>
	<description>Treetank is a fully java-implemented native XML-Database. 
	It supports cursorlike access to all nodes stored in the tree plus XPath 2.0 as query engine. Furthermore, Treetank is completely versionable. That means that each update-operation results in a new version. 
	These versions can be easily retrieved within the database itself.</description>
	<url>http://www.informatik.uni-konstanz.de/arbeitsgruppen/disy/forschung/treetank/</url>
	<scm>
		<connection>scm:svn:https://svn.uni-konstanz.de/disy/Treetank/trunk</connection>
		<developerConnection>scm:svn:https://svn.uni-konstanz.de/disy/Treetank/trunk</developerConnection>
		<url>https://svn.uni-konstanz.de/disy/Treetank</url>
	</scm>
	<distributionManagement>
		<repository>
			<id>disyInternal</id>
			<url>http://xen5.disy.inf.uni-konstanz.de:8080/archiva/repository/disyInternal/</url>
			<uniqueVersion>false</uniqueVersion>
		</repository>
		<snapshotRepository>
			<id>disyInternalSnapshot</id>
			<url>http://xen5.disy.inf.uni-konstanz.de:8080/archiva/repository/disyInternalSnapshot/</url>
			<uniqueVersion>false</uniqueVersion>
		</snapshotRepository>
	</distributionManagement>
	<modules>
		<module>parent</module>
		<module>bundles/treetank-core</module>
		<module>bundles/treetank-jax-rx</module>
		<module>bundles/treetank-saxon</module>
	</modules>
